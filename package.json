{
  "name": "felag",
  "version": "1.0.0",
  "description": "Website for Felag Club",
  "author": "Eduard Sadrutdinov",
  "license": "",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "styles": "gulp ststaryles",
    "start": "gulp",
    "optimize": "gulp optimize"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
